	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int n1 = sc.nextInt();
		
		int n2 = sc.nextInt();
		
		int result = n1 + n2;
		
		System.out.printf("A soma de $d e $d Ã© %d.", n1, n2, result);
		
		
		sc.close();
		
		
	}

}
